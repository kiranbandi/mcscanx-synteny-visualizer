{
    "name": "mcscanx-synteny-visualizer",
    "version": "1.0.0",
    "description": "UofS HCI synteny visualization tool for mcscanx collinearity files",
    "main": "src/app.js",
    "scripts": {
        "start": "webpack-dev-server --config ./build-config/webpack.config.js --watch-poll",
        "build": "webpack --config ./build-config/webpack.prod.config.js",
