    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-minify-webpack-plugin": "^0.2.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "gh-pages": "^0.12.0",
        "html-loader": "^0.5.5",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
